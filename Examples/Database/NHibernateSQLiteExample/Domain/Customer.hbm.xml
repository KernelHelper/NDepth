<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="NDepth.Examples.Database.NHibernateSQLiteExample" namespace="NDepth.Examples.Database.NHibernateSQLiteExample.Domain">
  <class name="Customer" table="Customer">
    <id name="Id">
      <generator class="guid.comb" />
    </id>
    <property name="FirstName" />
    <property name="LastName" />
    <property name="Address" />
    <property name="Email" />
    <property name="Phone" />
  </class>
</hibernate-mapping>